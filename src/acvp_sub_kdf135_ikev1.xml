                    "min": 224,
                    "max": 8192,
                    "increment": 1
                }
            ],
            "hashAlg": [
                "SHA-1",
                "SHA2-224",
                "SHA2-256",
                "SHA2-384",
                "SHA2-512"
            ]
        },
        {
            "authenticationMethod": "psk",
            "initiatorNonceLength": [
                {
                    "min": 64,
                    "max": 2048,
                    "increment": 1
                }
            ],
            "responderNonceLength": [
                {
                    "min": 64,
                    "max": 2048,
                    "increment": 1
                }
            ],
            "preSharedKeyLength": [
                {
                    "min": 8,
                    "max": 8192,
                    "increment": 1
                }
            ],
            "diffieHellmanSharedSecretLength": [
                {
                    "min": 224,
                    "max": 8192,
                    "increment": 1
                }
            ],
            "hashAlg": [
                "sha-1",
                "SHA2-224",
                "SHA2-256",
                "SHA2-384",
                "SHA2-512"
            ]
        },
        {
            "authenticationMethod": "pke",
            "initiatorNonceLength": [
                {
                    "min": 64,
                    "max": 2048,
                    "increment": 1
                }
            ],
            "responderNonceLength": [
                {
